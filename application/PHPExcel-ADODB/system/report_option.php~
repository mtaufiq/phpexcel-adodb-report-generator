<?php
$redirect_string = $GLOBALS['base_url'];

echo "global".$GLOBALS['base_url'];

/*GET ALL PARAMETERS*/
while ($rep_param = current($_POST)) {
    
    $rep_key = key($_POST);

    	$redirect_string .= "".$rep_key."=".$rep_param."&";		

    next($_POST);    
}

echo $redirect_string;

//header('Location: '.$redirect_string);
?>
